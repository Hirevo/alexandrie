[dependencies]
bitflags = "1.2.1"
cfg-if = "0.1.2"
ena = "0.14"
indexmap = "1"
lazy_static = "1"
libc = "0.2"
log = "0.4"
measureme = "0.7.1"
rustc-hash = "1.0.1"
stable_deref_trait = "1.0.0"
stacker = "0.1.9"

[dependencies.crossbeam-utils]
features = ["nightly"]
version = "0.7"

[dependencies.jobserver_crate]
package = "jobserver"
version = "0.1.13"

[dependencies.once_cell]
features = ["parking_lot"]
version = "1"

[dependencies.parking_lot]
features = ["nightly"]
version = "0.10"

[dependencies.rayon]
package = "rustc-rayon"
version = "0.3.0"

[dependencies.rayon-core]
package = "rustc-rayon-core"
version = "0.3.0"

[dependencies.rustc_graphviz]
package = "rustc-ap-rustc_graphviz"
version = "664.0.0"

[dependencies.rustc_index]
package = "rustc-ap-rustc_index"
version = "664.0.0"

[dependencies.rustc_serialize]
package = "rustc-ap-rustc_serialize"
version = "664.0.0"

[dependencies.smallvec]
features = ["union", "may_dangle"]
version = "1.0"

[lib]
doctest = false
path = "lib.rs"

[package]
authors = ["The Rust Project Developers"]
description = "Automatically published version of the package `rustc_data_structures` in the rust-lang/rust repository from commit 3ddf48053e83f4949d00dcaf6eb8d9e28fc6bb95 The publishing script for this crate lives at: https://github.com/alexcrichton/rustc-auto-publish\n            "
edition = "2018"
license = "MIT / Apache-2.0"
name = "rustc-ap-rustc_data_structures"
repository = "https://github.com/rust-lang/rust"
version = "664.0.0"
[target."cfg(windows)".dependencies.winapi]
features = ["fileapi", "psapi"]
version = "0.3"
